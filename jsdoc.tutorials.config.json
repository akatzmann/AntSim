{
    "plugins": [],
    "recurseDepth": 10,
	"sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
	"plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
	"opts": {
        "template": "templates/default",
        "encoding": "utf8",
		"destination": "./tutorials_doc/",	
	    "recurse": true,
        "tutorials": "tutorials" // same as -u path/to/tutorials
 }
}

